class SumTest4 
{
	public static void main(String[] args) 
	{
		int i;
		
		for (i = 1; i <= 10 ; i++ )
		{
			if (i == 5) return;

			System.out.print(i + " ");
			
		}
		System.out.println("\n프로그램을 종료합니다.");
	}
}
